    {% for p in pages  %}
        <url>
            <loc>https://agronizer.ru/{{ p.slug }}</loc>
            <lastmod>2016-07-20T00:00:00+01:00</lastmod>
            <priority>1.0</priority>
            <changefreq>daily</changefreq>
        </url>
    {% endfor %}
    {% for publication in publications  %}
        {% if publication.type == 0 %}
            {% set type = 'article' %}
        {% elseif publication.type == 2 %}
            {% set type = 'study' %}
        {% else %}
            {% set type = 'new' %}
        {% endif %}
        <url>
            <loc>https://agronizer.ru/publications/{{ type }}/{{ publication.slug ? publication.slug : publication.id }}</loc>
            <lastmod>{{ publication.created | date('Y-m-d') }}T12:24:14+01:00</lastmod>
            <priority>0.8</priority>
            <changefreq>daily</changefreq>
        </url>
    {% endfor %}
    {% for event in events  %}
        {#{% if publication.type == 0 %}#}
            {#{% set type = 'article' %}#}
        {#{% elseif publication.type == 2 %}#}
            {#{% set type = 'study' %}#}
        {#{% else %}#}
            {#{% set type = 'new' %}#}
        {#{% endif %}#}
        <url>
            <loc>https://agronizer.ru/event/{{ event.slug ? event.slug : event.id }}</loc>
            <lastmod>{{ event.created | date('Y-m-d') }}T12:24:14+01:00</lastmod>
            <priority>0.8</priority>
            <changefreq>daily</changefreq>
        </url>
    {% endfor %}
    {% for c in calendar  %}
        <url>
            <loc>https://agronizer.ru/medical-calendar/now/{{ c.id }}</loc>
            <lastmod>2016-07-20T00:00:00+01:00</lastmod>
            <priority>1.0</priority>
            <changefreq>daily</changefreq>
        </url>
    {% endfor %}
    {% for s in standarts  %}
        <url>
            <loc>https://agronizer.ru/federal-standards/{{ s.id }}</loc>
            <lastmod>2016-07-20T00:00:00+01:00</lastmod>
            <priority>1.0</priority>
            <changefreq>daily</changefreq>
        </url>
    {% endfor %}
</urlset>